/// https://www.erisu.net/?PAap=%E3%83%95%E3%82%A3%E3%83%9C%E3%83%8A%E3%83%83%E3%83%81%E6%95%B0%E5%88%97
/// フィボナッチ数列を羅列
fn main() {
    let len = 20; // 表示させる個数
    let mut n = [1, 0];
    for x in 0..len {
        let y = n[x % 2];
        print!("{} ", y);
        n[(x + 1) % 2] = n[0] + n[1]; 
    }
}
