/// https://www.erisu.net/?PAap=%E3%83%A9%E3%82%A4%E3%83%95%E3%82%BF%E3%82%A4%E3%83%A0%E3%83%91%E3%83%A9%E3%83%A1%E3%83%BC%E3%82%BF
/// 参照フィールドを持つ構造体のライフタイムパラメータ記述
#[derive(Debug)]
struct St<'a, 'b> {
    name: &'a str,
    lang: &'b str,
}
impl<'a, 'b> St<'a, 'b> {
    fn new(name: &'a &str, lang: &'b &str) -> St<'a, 'b> {
        St { name, lang }
    }
}

fn main() {
    let x = St::new(&"xmen", &"Rust");
    println!("name:{}, lang:{}", x.name, x.lang);
}
